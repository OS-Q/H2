{
  "build": {
    "core": "sduino",
    "extra_flags": "-DSTM8S_BLUE -DSTM8S003",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "stm8s103f3p6",
    "variant": "standard"
  },
  "debug": {
    "openocd_target": "stm8s",
    "tools": {
      "stlink": {
        "default": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/stlink.cfg",
            "-f",
            "scripts/target/stm8s.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "arduino",
    "native"
  ],
  "upload": {
    "maximum_ram_size": 1024,
    "maximum_size": 8192,
    "protocol": "stlinkv2",
    "protocols": [
      "serial",
      "stlink",
      "stlinkv2"
    ]
  },
  "name": "STM8S003F3",
  "url": "http://www.OS-Q.com/P14/STM8S003F3",
  "vendor": "OS-Q"
}
