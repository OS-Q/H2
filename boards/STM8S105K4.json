{
  "build": {
    "core": "sduino",
    "extra_flags": "-DSTM8S_BLACK -DSTM8S105",
    "f_cpu": "16000000L",
    "cpu": "stm8",
    "mcu": "stm8s105k4t6",
    "variant": "STM8S105K4"
  },
  "debug": {
    "openocd_target": "stm8s",
    "tools": {
      "stlink": {
        "default": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/interface/stlink.cfg",
            "-f",
            "scripts/target/stm8s.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        },
        "onboard": true
      }
    }
  },
  "frameworks": [
    "arduino",
    "native"
  ],
  "upload": {
    "maximum_ram_size": 2048,
    "maximum_size": 16384,
    "protocol": "stlinkv2",
    "protocols": [
      "serial",
      "stlink",
      "stlinkv2"
    ]
  },
  "name": "STM8S105K4",
  "url": "http://www.OS-Q.com/P14/STM8S105K4",
  "vendor": "OS-Q"
}
