{
  "name": "P14",
  "title": "Platform P14 (STM8S)",
  "description": "STMicroelectronics base-line 8-bit microcontroller family STM8S.",
  "homepage": "https://www.st.com/en/microcontrollers/stm8-8-bit-mcus.html",
  "license": "Apache-2.0",
  "version": "1.0.4",
  "engines": {
    "platformio": "^5"
  },
  "keywords": [
      "dev-platform",
      "OS-Q",
      "STM8S",
      "8bit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P14.git"
  },
  "frameworks": {
    "arduino": {
      "package": "A14",
      "script": "builder/frameworks/arduino.py"
    },
    "native": {
      "package": "E14",
      "script": "builder/frameworks/native.py"
    }
  },
  "packages": {
    "A14": {
      "type": "framework",
      "optional": true,
      "owner": "OS-Q",
      "version": "https://github.com/OS-Q/A14.git"
    },
    "E14": {
      "type": "framework",
      "optional": true,
      "owner": "OS-Q",
      "version": "https://github.com/OS-Q/E14.git"
    },
    "toolchain-sdcc": {
      "type": "toolchain",
      "version": "~1.30804.0"
    },
    "tool-stm8binutils": {
      "type": "uploader",
      "version": "~0.230.0"
    },
    "tool-stm8tools": {
      "type": "uploader",
      "optional": true,
      "version": "~0.40.181218"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    }
  }
}
