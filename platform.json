{
  "name": "P14",
  "title": "Platform P14 (STM8S)",
  "description": "STMicroelectronics 8-bit microcontroller STM8S.",
  "homepage": "https://www.st.com/en/microcontrollers/stm8-8-bit-mcus.html",
  "license": "Apache-2.0",
  "version": "1.0.3",
  "engines": {
    "platformio": "^5"
  },
  "keywords": [
      "dev-platform",
      "OS-Q",
      "STM8S",
      "8bit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P14.git"
  },
  "frameworks": {
    "arduino": {
      "package": "framework-arduinoststm8",
      "script": "builder/frameworks/arduino.py"
    },
    "spl": {
      "package": "framework-ststm8spl",
      "script": "builder/frameworks/spl.py"
    },
    "ino": {
      "package": "A14A",
      "script": "builder/extend/A14A.py"
    },
    "native": {
      "package": "E14A",
      "script": "builder/extend/E14A.py"
    }
  },
  "packages": {
    "A14A": {
      "type": "framework",
      "optional": true,
      "owner": "OS-Q",
      "version": "https://github.com/OS-Q/A14A.git"
    },
    "E14A": {
      "type": "framework",
      "optional": true,
      "owner": "OS-Q",
      "version": "https://github.com/OS-Q/E14A.git"
    },
    "framework-arduinoststm8": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "0.40.181216"
    },
    "framework-ststm8spl": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "0.20301.181217"
    },
    "toolchain-sdcc": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~1.30804.10766"
    },
    "tool-stm8binutils": {
      "type": "uploader",
      "owner": "platformio",
      "version": "0.230.0"
    },
    "tool-stm8tools": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~0.40.181218"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~2.1000.0"
    }
  }
}
